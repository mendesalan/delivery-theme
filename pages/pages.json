{
  "templates": {
    "product": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/ProductContextProvider",
      "props": {
        "elements": [
          "details",
          "customizer"
        ]
      },
      "extensions": {
        "details": {
          "component": "vtex.product-details/ProductDetails"
        },
        "customizer": {
          "component": "vtex.product-customizer/ProductCustomizer"
        }
      }
    }
  },
  "pages": {
    "store/product": [
      {
        "name": "Default",
        "template": "product"
      }
    ]
  }
}
