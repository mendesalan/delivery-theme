{
  "routes": {
    "store/home": {
      "path": "/"
    },
    "store/order": {
      "path": "/order",
      "context": "HomeContextProvider"
    }
  },
  "templates": {
    "home": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": ["address"]
      },
      "extensions": {
        "address": {
          "component": "vtex.address-locator/index"
        }
      }
    },
    "order": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": ["greeting", "carousel", "last-order", "shelf"]
      },
      "extensions": {
        "greeting": {
          "component": "Greeting"
        },
        "carousel": {
          "component": "vtex.carousel/Carousel"
        },
        "shelf": {
          "component": "vtex.shelf/Shelf"
        }
      }
    }
  },
  "pages": {
    "store/home": [
      {
        "name": "Default",
        "template": "home"
      }
    ],
    "store/order": [
      {
        "name": "Order",
        "template": "order"
      }
    ]
  }
}
